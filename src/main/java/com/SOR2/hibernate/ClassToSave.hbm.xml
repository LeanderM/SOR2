<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SOR2.hibernate">
	<class name="ClassToSave" table="TEMP">
		<id name="id" column="TEMP_ID">
			<generator class="native" />
		</id>
		<property name="date" type="timestamp" column="TEMP_DATE"/>
		<property name="title" column="TEMP_TITLE"/>
	</class>
</hibernate-mapping>